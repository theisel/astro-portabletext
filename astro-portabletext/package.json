{
  "name": "astro-portabletext",
  "version": "0.12.0",
  "type": "module",
  "description": "A flexible and customizable library for rendering Portable Text content in your Astro projects",
  "keywords": [
    "astro",
    "withastro",
    "astro-component",
    "portable-text",
    "typescript",
    "ui"
  ],
  "author": "Tom Theisel <tom.theisel@gmail.com>",
  "license": "ISC",
  "exports": {
    ".": "./lib/index.ts",
    "./components": "./lib/components.ts",
    "./types": "./lib/types.ts",
    "./utils": {
      "types": "./lib/utils.d.ts",
      "import": "./lib/utils.ts",
      "default": "./lib/utils.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "lib/*"
      ]
    }
  },
  "files": [
    "CHANGELOG.md",
    "components",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/theisel/astro-portabletext.git",
    "directory": "astro-portabletext"
  },
  "bugs": {
    "url": "https://github.com/theisel/astro-portabletext/issues"
  },
  "homepage": "https://github.com/theisel/astro-portabletext#readme",
  "scripts": {
    "check": "astro check --root components"
  },
  "dependencies": {
    "@portabletext/toolkit": "^5.0.1",
    "@portabletext/types": "^4.0.1"
  },
  "peerDependencies": {
    "astro": ">=4.6.0"
  },
  "engines": {
    "node": ">=20.19 <22 || >=22.12"
  }
}
