---
import Layout from "../../layouts/Default.astro";
import { PortableText } from "astro-portabletext";

const blocks = [
  {
    _type: "block",
    children: [
      {
        _type: "span",
        text: "bold",
        marks: ["strong"],
      },
    ],
  },
];
---

<Layout>
  <PortableText value={blocks} />
</Layout>
