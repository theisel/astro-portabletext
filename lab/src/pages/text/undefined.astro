---
import { PortableText } from "astro-portabletext";
import Layout from "../../layouts/Default.astro";

const blocks = [
  {
    _type: "block",
    children: [
      {
        _type: "span",
        text: "hello world",
      },
    ],
  },
];
---

<Layout>
  <PortableText value={blocks} components={{ text: undefined }} />
</Layout>
