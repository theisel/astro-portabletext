    "@typescript-eslint/parser": "^5.25.0",
    "astro": "1.0.0-beta.31",
    "eslint": "^8.16.0",
    "husky": "^8.0.1",
    "prettier": "^2.6.2",
    "prettier-plugin-astro": "0.0.12",
